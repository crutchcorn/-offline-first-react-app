		"@tanstack/react-query-persist-client": "^5.17.19",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"astro": "^4.2.4",
		"houseform": "^1.11.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^9.1.0",
		"react-router-dom": "^6.21.3",
		"redux-persist": "^6.0.0",
		"superjson": "^2.2.1",
